    Obtains Emirates Skywards bonus miles from your personal account at https://www.skywards.com.

    If you don't have Skywards number, please join here: https://www.skywards.com/join_now_personal.aspx
  </description>
  <author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>moment.min.js</js>
    <js>library.js</js>
    <js>main.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="balance" name="Skywards Miles" units=""/>
    <counter id="tier_balance" name="Tier Miles" units=""/>
    <counter id="fio" name="Name" type="text"/>
    <counter id="num" name="Skywards No" type="text"/>
    <counter id="till" name="Miles expiring date" type="time" format="dd/MM/yyyy"/>
    <counter id="burn" name="Miles expiring" units=""/>
  </counters>
  <keywords>
  </keywords>
  <type>
    bonus
  </type>
  <country>
    ae
  </country>
