<?xml version="1.0" encoding="utf-8"?>
<provider>
  <!-- Флаг отключает загрузку rhino.env.js на сервере, обязательно! -->
  <api flags="no_browser" />
  <id version="3">ab-hosting-jino</id>
  <name>Джино</name>
  <description>
     Получает значения из личного кабинета Хостинг-провайдера Джино (http://cp.jino.ru/)
  </description>
  <author>Stark &lt;support@starksoft.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>library.js</js>
    <js>main.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="balance" name="Баланс" units=" р"/>
	<counter id="deadline" name="Дата отключения" type="time" format="dd/MM/yyyy"/>
	<counter type= "text" id="state" name="Статус"/>
	<counter id="monthly_fee" name="Оплата в месяц" units=" р"/>
	<counter id="daily_fee" name="Оплата в день" units=" р"/>
    <counter id="storage_used" name="Дисковое простарнство - использовано" units=" мб"/>
	<counter type= "text" id="storage_percent" name="Дисковое простарнство - процент свободного места" units=" %"/>
  </counters>
  <keywords>
    Джино
  </keywords>
  <type>
    hosting
  </type>
  <country>
    ru
  </country>
</provider>
