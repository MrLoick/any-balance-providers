<?xml version="1.0" encoding="utf-8"?>
<provider>
	<api flags="no_browser"/>
	<id version="2">ab-internet-vsevnet</id>
	<name>Всевнет (Всеволожск)</name>
	<description>
		Получает баланс и информацию о тарифном плане для всеволожского интернет-провайдера Всевнет (http://vsevnet.ru). Информация берется из личного кабинета http://user.vsevnet.ru.
	</description>
	<author>AnyBalance Team &lt;info@anybalance.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" р"/>
		<counter id="trafficIn" name="Вх.трафик в текущем месяце" units=" Гб"/>
		<counter id="trafficOut" name="Исх.трафик в текущем месяце" units=" Гб"/>
		<counter id="charge" name="Следующее списание абон.платы" type="time" format="dd/MM/yyyy"/>
		<counter id="status" name="Статус интернета" type="text"/>
		<counter id="abon" name="Абон. плата за интернет" units=" р"/>
		<counter id="agr" name="Номер договора" type="text"/>
		<counter id="fio" name="ФИО" type="text"/>
	</counters>
	<keywords></keywords>
	<type>internet</type>
	<country>ru</country>
</provider>
