<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen>
	<EditTextPreference
		title="Регистрационный номер"
		key="login"
		summary="||{@s}"
		obligatory="true"
		dialogTitle="Регистрационный номер"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите регистрационный номер.">
	</EditTextPreference>
	<EditTextPreference
		title="Дата рождения"
		key="birthdate"
		summary="||{@s}"
		obligatory="true"
		dialogTitle="Дата рождения"
		positiveButtonText="ОК"
		negativeButtonText="Отмена"
		dialogMessage="Введите дату рождения в формате 2013-02-10.">
	</EditTextPreference>
    <ListPreference 
        key="type" 
        title="Регион"
