<?xml version="1.0" encoding="utf-8"?>
<provider>
  <id major_version="1.0" version="2">ab-weather-gismeteo-ru</id>
  <name>Gismeteo.ru</name>
  <description>
    Получает "Погоду за окном" для выбранного города.
  </description>
  <author>iReset &lt;develop_ag@mail.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>main.js</js>
    <js>common_functions.js</js>
  </files>
  <counters>
    <counter id="temperature" name="Температура" units="°C"/>
    <counter id="atmosphericConditions" name="Атмосферные явления" type="text"/>
    <counter id="wind" name="Ветер" type="text"/>
    <counter id="pressure" name="Атмосферное давление" units="мм.рт.ст."/>
    <counter id="humidity" name="Влажность" units="%"/>
    <counter id="time" name="Время обновления" type="time"/>
  </counters>
  <keywords>
    прогноз, погода, карты, осадки, ветер, влажность, давление, gismeteo, гисметео
  </keywords>
  <type>
    weather
  </type>
</provider>
