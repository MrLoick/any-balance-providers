<?xml version="1.0" encoding="utf-8"?>
<provider>
  <id version="1">ab-internet-dialog</id>
  <name>Диалог (Калининград)</name>
  <description>
    Получает баланс и информацию о тарифном плане для калининградского интернет провайдера Диалог. Информация берется из личного кабинета https://stats.tis-dialog.ru.
  </description>
  <author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>main.js</js>
  </files>
  <counters>
    <counter id="balance" name="Баланс" units="р"/>
    <counter id="trafficExt" name="Внешний трафик" units="Gb"/>
    <counter id="trafficCity" name="Городской трафик" units="Gb"/>
    <counter id="turbo" name="Турбо трафик" units="Gb"/>
    <counter id="turbo_bought" name="Купленный турбо трафик" units="Gb"/>
    <counter id="status" name="Состояние" type="text"/>
    <counter id="licschet" name="Лицевой счет" type="text"/>
    <counter id="userName" name="ФИО" type="text"/>
    <counter id="trafficExtIn" name="Внешний трафик (вх)" units="Gb"/>
    <counter id="trafficExtOut" name="Внешний трафик (исх)" units="Gb"/>
    <counter id="trafficCityIn" name="Городской трафик (вх)" units="Gb"/>
    <counter id="trafficCityOut" name="Городской трафик (исх)" units="Gb"/>
  </counters>
  <keywords>
    интернет, калининград
  </keywords>
  <type>
    internet
  </type>
</provider>
