	<p>If you don't have Vodafone personal account, please register here: <a href="https://www.vodafone.com.eg/vodafoneportalWeb/en/MyVodafone_Register">https://www.vodafone.com.eg/vodafoneportalWeb/en/MyVodafone_Register</a></p>
	]]></description>	
	<author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Balance" units=" L.E."/>
		<counter id="fio" name="Name" type="text"/>
		<counter id="phone" name="Phone" type="text"/>
		<counter id="accnum" name="Account number" type="text"/>
		<counter id="traf_used" name="Data used" units=" Mb"/>
		<counter id="traf_left" name="Date left" units=" Mb"/>
		<counter id="traf_total" name="Data total" units=" Mb"/>
	</counters>
