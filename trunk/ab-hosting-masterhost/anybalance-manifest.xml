<?xml version="1.0" encoding="utf-8"?>
<provider>
  <id version="1">ab-hosting-masterhost</id>
  <name>Masterhost (хостинг)</name>
  <description>
    Получает баланс и информацию о доменах у хостинг провайдера Мастерхост (http://masterhost.ru).

    Информация берется из личного кабинета https://cp.masterhost.ru. Может получать информацию по 4 доменам одновременно.
  </description>
  <author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>main.js</js>
    <js>library.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="balance" name="Баланс" units="р"/>
    <counter id="userName" name="Имя" type="text"/>
    <counter id="till" name="Прогноз отключения" type="time" format="dd/MM/yyyy"/>
    <counter id="lastop" name="Последняя операция" type="text"/>
    <counter id="lastopdate" name="Дата последней операции" type="time"/>
    <counter id="lastopinit" name="Инициатор последней операции" type="text"/>
    <counter id="lastopip" name="IP последней операции" type="text"/>
    <counter id="domain" name="Домен" type="text"/>
    <counter id="domain_till" name="До" type="time" format="dd/MM/yyyy"/>
    <counter id="domain1" name="Домен 2" type="text"/>
    <counter id="domain_till1" name="До 2" type="time" format="dd/MM/yyyy"/>
    <counter id="domain2" name="Домен 3" type="text"/>
    <counter id="domain_till2" name="До 3" type="time" format="dd/MM/yyyy"/>
    <counter id="domain3" name="Домен 4" type="text"/>
    <counter id="domain_till3" name="До 4" type="time" format="dd/MM/yyyy"/>
    <counter id="balance_ue" name="Баланс уе" units="уе"/>
  </counters>
  <keywords>
    интернет регистратор hosting
  </keywords>
  <type>
    hosting
  </type>
  <country>
    ru
  </country>
</provider>
