<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id major_version="2" version="18">ab-telephony-skype</id>
	<api flags="no_browser" />
	<name>Skype</name>
	<description html="true"><![CDATA[
	<p>Предоставляет информацию о текущем балансе, валюте баланса в Skype (http://skype.com).</p>
	]]></description>
	<author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Баланс" units=" {@currency}"/>
		<counter id="currency" name="Валюта" type="text"/>
		<counter id="inactivein" name="Деактивация средств через" units=" дн."/>
		<counter id="inactivedate" name="Дата деактивации средств" type="time" format="dd/MM/yyyy"/>
		<counter id="subscriptions" name="Число подписок"/>
		<counter id="minsLeft" name="Остаток минут" units=" мин"/>
		<counter id="landline" name="Звонки за последние 30 дней" units=" мин"/>
		<counter id="sms" name="SMS за последние 30 дней"/>
		<counter id="wifi" name="Skype WiFi за последние 30 дней" units=" мин"/>
	</counters>
	<keywords>Скайп, IP-телефония</keywords>
	<type>telephony</type>
</provider>