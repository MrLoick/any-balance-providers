<?xml version="1.0" encoding="utf-8"?>
<provider>
  <!-- Флаг отключает загрузку rhino.env.js на сервере, обязательно! -->
  <api flags="no_browser" />
  <id version="4">ab-cell-pildyk</id>
  <name>Pildyk</name>
  <description>
    Получает баланс для Pildyk (www.pildyk.lt)
  </description>
  <author>Ilya Laptev &lt;ia.laptev@gmail.com&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>library.js</js>
    <js>main.js</js>
    <history>history.xml</history>
  </files>
  <counters>
    <counter id="balance" name="Баланс" units=" lt"/>
    <counter id="balanceExpire" name="Дата сгорания баланса" type="time" format="dd/MM/yyyy"/>
    <counter id="name" name="Владелец" type="text"/>    
    <counter id="telnum" name="Номер телефона" type="text"/>    
  </counters>
  <keywords>
    pildyk
  </keywords>
  <type>
    cell
  </type>
  <country>
    lt
  </country>
</provider>
