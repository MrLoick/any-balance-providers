<?xml version="1.0" encoding="utf-8"?>
<provider>
  <id version="2">ab-internet-rostelekom-kamchatka</id>
  <name>Интернет Ростелеком (Камчатка)</name>
  <description>
    Получает данные из ИССА Камчатского филиала ОАО "Ростелеком": баланс, счетчики трафика
  </description>
  <author>Dmitry Kochin &lt;dco@mail.ru&gt;</author>
  <files>
    <icon>icon.png</icon>
    <preferences>preferences.xml</preferences>
    <js>api:jquery.min.js</js> <!-- Используем библиотеку, которая и так входит в AnyBalance -->
    <js>main.js</js>
  </files>
  <counters>
    <counter id="balance" name="Баланс" units="р"/>
    <counter id="traffic_local_in" name="Входящий локальный трафик" suffix="Мб"/>
    <counter id="traffic_global_out" name="Исходящий внешний трафик" suffix="Мб"/>
    <counter id="traffic_global_in" name="Входящий внешний трафик" suffix="Мб"/>
    <counter id="traffic_included" name="Предоплаченый трафик" suffix="Мб"/>
    <counter id="traffic_kamchatka" name="Безлимитная Камчатка" suffix="Гб"/>
    <counter id="time_off" name="Время отключения" suffix="дн"/>
    <counter id="average_speed" name="Скорость расходования" suffix="р"/>
    <counter id="agreement" name="Номер договора" type="text"/>
    <counter id="username" name="ФИО" type="text"/>
 </counters>
  <keywords>
    провайдер интернет камчатский край
  </keywords>
  <type>
    internet
  </type>
</provider>
