<?xml version="1.0" encoding="utf-8"?>
<provider>
	<id major_version="1" version="1">ab-money-rikslunchen</id>
	<api flags="no_browser" />
	<name>Rikslunchen</name>
	<description html="true"><![CDATA[
	<p>Reads the balance of Rikslunchen money card from Rikslunchen account (http://www.rikslunchen.se)</p>
	]]></description>
	<author>valeravi &lt;valeravi@vi-soft.com.ua&gt;</author>
	<files>
		<icon>icon.png</icon>
		<preferences>preferences.xml</preferences>
		<js>library.js</js>
		<js>main.js</js>
		<history>history.xml</history>
	</files>
	<counters>
		<counter id="balance" name="Balance" units=" {@currency}"/>
		<counter id="currency" name="Currency" type="text"/>
		<counter id="topuplast" name="Last top-up" type="time" format="dd/MM/yyyy"/>
		<counter id="transactions" name="Transactions count"/>
	</counters>
	<keywords>subsidized, lunch, food, subventionerar, lunchen</keywords>
	<type>money</type>
	<country>se</country>
</provider>